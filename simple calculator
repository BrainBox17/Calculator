    public void arithmetic_operation(){
        switch(calculation){
//            ADDITION
            case 1: ans = num + Integer.parseInt(jTextField1.getText());
           jet = (int) ans;
            jTextField1.setText(Integer.toString(jet));
            break;
//            SUBTRACTION
            case 2: ans = num - Integer.parseInt(jTextField1.getText());
          jet = (int) ans;
            jTextField1.setText(Integer.toString(jet));
            break;
            
//            MULTIPLICATION
            case 3: ans = num * Integer.parseInt(jTextField1.getText());
       jet = (int) ans;
            jTextField1.setText(Integer.toString(jet));
            break;
            
//            DIVISION
            case 4: ans = num / Double.parseDouble(jTextField1.getText());
            jTextField1.setText(Double.toString(ans));
            break;
        }
    }
    
